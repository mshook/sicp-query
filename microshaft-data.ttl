# Property Definitions

:hasAddress a rdf:Property ;
    rdfs:label "has address" ;
    rdfs:domain foaf:Person ;
    rdfs:range :Address .

:hasJob a rdf:Property ;
    rdfs:label "has job" ;
    rdfs:domain foaf:Person ;
    rdfs:range :JobRole .

:salary a rdf:Property ;
    rdfs:label "salary" ;
    rdfs:domain foaf:Person ;
    rdfs:range xsd:integer .

:city a rdf:Property ;
    rdfs:label "city" ;
    rdfs:domain :Address ;
    rdfs:range xsd:string .

:street a rdf:Property ;
    rdfs:label "street" ;
    rdfs:domain :Address ;
    rdfs:range xsd:string .

:streetNumber a rdf:Property ;
    rdfs:label "street number" ;
    rdfs:domain :Address ;
    rdfs:range xsd:integer .

:jobTitle a rdf:Property ;
    rdfs:label "job title" ;
    rdfs:domain :JobRole ;
    rdfs:range xsd:string .

:canDoJob a rdf:Property ;
    rdfs:label "can do job" ;
    rdfs:comment "Indicates that a person with one job role can also perform another job role" ;
    rdfs:domain :JobRole ;
    rdfs:range :JobRole .

# Class Definitions

:Address a rdfs:Class ;
    rdfs:label "Address" ;
    rdfs:comment "A physical address with city, street, and street number" .

:JobRole a rdfs:Class ;
    rdfs:label "Job Role" ;
    rdfs:comment "A job position or role within the organization" .
